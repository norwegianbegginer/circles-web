{
  "name": "circles-web",
  "version": "1.0.0-alpha.1",
  "private": true,
  "homepage": "https://project-circles.fun",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "csstype": "^3.0.6",
    "firebase": "^7.15.1",
    "gsap": "^3.5.1",
    "notistack": "^0.9.17",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-littera": "^1.2.2",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "concurrently \"yarn start:app\" \"yarn start:storybook\"",
    "build": "concurrently \"yarn build:app\" \"yarn build:storybook\"",
    "start:app": "react-scripts start",
    "build:app": "react-scripts build -- --stats",
    "start:storybook": "start-storybook -p 6006 -s public",
    "build:storybook": "build-storybook -s public",
    "dev": "yarn start:app",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "report": "webpack-bundle-analyzer ./build/bundle-stats.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/node-logger": "^6.1.20",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.20",
    "@storybook/theming": "^6.1.20",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.5",
    "compression-webpack-plugin": "^7.1.2",
    "concurrently": "^6.0.0",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
